{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api",  // Removed the trailing slash
      "methods": ["POST"],
      "dest": "app.py"
    },
    {
      "src": "/ui",
      "dest": "app.py"
    },
    {
      "src": "/",
      "dest": "app.py"
    }
  ]
}
